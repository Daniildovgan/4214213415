#include <iostream>
#include <vector>
using namespace std;

template <typename T>
void addRow(vector<vector<T>>& matrix, const vector<T>& newRow) {
    matrix.push_back(newRow);
}

int main() {
    vector<vector<int>> matrix = {
        {1, 2, 3},
        {4, 5, 6},
        {7, 8, 9}
    };

    vector<int> newRow = {10, 11, 12};
    addRow(matrix, newRow);

    for (const auto& row : matrix) {
        for (const auto& elem : row) {
            cout << elem << " ";
        }
        cout << endl;
    }

    return 0;
}
